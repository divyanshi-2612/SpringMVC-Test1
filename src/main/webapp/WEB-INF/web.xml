<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SearchStudent</servlet-name>
  	<display-name>SearchStudent</display-name>
  	<description></description>
  	<servlet-class>com.db.servlet.SearchStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertStudent</servlet-name>
  	<display-name>InsertStudent</display-name>
  	<description></description>
  	<servlet-class>com.db.servlet.InsertStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteStudent</servlet-name>
  	<display-name>DeleteStudent</display-name>
  	<description></description>
  	<servlet-class>com.db.servlet.DeleteStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateStudent</servlet-name>
  	<display-name>UpdateStudent</display-name>
  	<description></description>
  	<servlet-class>com.db.servlet.UpdateStudent</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SearchStudent</servlet-name>
  	<url-pattern>/SearchStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertStudent</servlet-name>
  	<url-pattern>/InsertStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteStudent</servlet-name>
  	<url-pattern>/DeleteStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateStudent</servlet-name>
  	<url-pattern>/UpdateStudent</url-pattern>
  </servlet-mapping>
</web-app>
